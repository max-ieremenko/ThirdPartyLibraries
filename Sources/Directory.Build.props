<Project>
  <PropertyGroup>
    <TargetFrameworks>netcoreapp3.1;net5.0</TargetFrameworks>
    <OutputType>Library</OutputType>
	<GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <WarningsAsErrors />
	
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>..\ThirdPartyLibraries.snk</AssemblyOriginatorKeyFile>
    
    <CodeAnalysisRuleSet>..\StyleCope.ruleset</CodeAnalysisRuleSet>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="..\GlobalAssemblyInfo.cs" Link="Properties\GlobalAssemblyInfo.cs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="StyleCop.Analyzers.Unstable" Version="1.2.0.333">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>
  
  <PropertyGroup>
    <NewtonsoftJsonVersion>12.0.3</NewtonsoftJsonVersion>
	<UnityVersion>5.11.3</UnityVersion>
	<MicrosoftNETTestSdkVersion>16.4.0</MicrosoftNETTestSdkVersion>
	<MoqVersion>4.13.1</MoqVersion>
	<NUnitVersion>3.12.0</NUnitVersion>
	<NUnit3TestAdapterVersion>3.16.0</NUnit3TestAdapterVersion>
	<ShouldlyVersion>3.0.2</ShouldlyVersion>
	<MockHttpVersion>6.0.0</MockHttpVersion>
  </PropertyGroup>
  
</Project>